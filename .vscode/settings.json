{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll": "explicit"
  },
  "eslint.ignoreUntitled": true,
  "eslint.validate": ["javascript", "typescript", "vue", "json"],
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "editor.formatOnSave": true
}
