{
  "compilerOptions": {
    "outDir": "dist",
    "types": ["node"],
    "rootDir": ".",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": false,
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "extends": "./tsconfig/index.json",
  "include": ["./eslint", "./prettier", "./tsconfig", "./unocss", "types", "./scripts"]
}
